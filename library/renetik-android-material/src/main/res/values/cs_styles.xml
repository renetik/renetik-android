<resources>

    <style name="CSContentEmpty" parent="CSWrap">
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:textAppearance">@style/CSTextOverline</item>
    </style>

    <style name="CSTabStripTop" parent="CSTabStripBottom">
        <item name="android:layout_gravity">top</item>
    </style>

    <style name="CSContentCardMatchFill" parent="CSContentCardMatchWrap">
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSPageTitleCard" parent="Widget.MaterialComponents.CardView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CSPageTitleMatchWrap" parent="CSMatchWrap">
        <item name="android:gravity">center</item>
        <item name="android:padding">3sp</item>
        <item name="android:textAppearance">@style/TextAppearance.MaterialComponents.Subtitle2
        </item>
    </style>

    <style name="CSTabStripBottom" parent="CSMatch">
        <item name="android:background">?attr/colorPrimaryVariant</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="android:textAppearance">@style/CSTextSubtitle1</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:layout_height">40dp</item>
    </style>

    <style name="CSContentCardMatchWrap" parent="CSContentCardWrap">
        <item name="android:layout_width">match_parent</item>
    </style>


    <style name="CSContentCardWrap" parent="Widget.MaterialComponents.CardView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginTop">3dp</item>
        <item name="android:layout_marginBottom">3dp</item>
    </style>

    <style name="CSGridItemCardMatchWrap" parent="Widget.MaterialComponents.CardView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_margin">1dp</item>
    </style>

    <style name="CSButtonFillWrap" parent="CSButtonMatchWrap">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSButtonMatchWrap" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CSButtonWrap" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CSVariantButtonFillWrap" parent="CSVariantButtonMatchWrap">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSVariantButtonMatchWrap" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CSVariantButtonWrap" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CSTextButtonWrap" parent="CSTextButtonMatchWrap">
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="CSTextButtonFillWrap" parent="CSTextButtonMatchWrap">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="CSTextButtonMatchWrap" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CSFloatingButtonBottomStartWrap" parent="CSFloatingButtonBottomEndWrap">
        <item name="android:layout_gravity">bottom|start</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginTop">0dp</item>
        <item name="android:layout_marginEnd">0dp</item>
    </style>

    <style name="CSFloatingButtonTopStartWrap" parent="CSFloatingButtonTopEndWrap">
        <item name="android:layout_gravity">top|start</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginBottom">0dp</item>
        <item name="android:layout_marginEnd">0dp</item>
    </style>

    <style name="CSFloatingButtonBottomEndWrap" parent="CSFloatingButtonTopEndWrap">
        <item name="android:layout_gravity">bottom|end</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:layout_marginEnd">16dp</item>
        <item name="android:layout_marginTop">0dp</item>
        <item name="android:layout_marginStart">0dp</item>
    </style>

    <style name="CSFloatingButtonTopEndWrap" parent="Widget.MaterialComponents.FloatingActionButton">
        <item name="android:layout_gravity">top|end</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:layout_marginEnd">16dp</item>
        <item name="android:layout_marginStart">0dp</item>
        <item name="android:layout_marginBottom">0dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CSIconButtonOutlinedWrap" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="iconPadding">0dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:paddingLeft">0dp</item>
        <item name="android:paddingRight">0dp</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:padding">12dp</item>
    </style>

    <style name="CSIconButtonWrap" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="strokeWidth">0dp</item>
        <item name="iconPadding">0dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:paddingLeft">0dp</item>
        <item name="android:paddingRight">0dp</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:padding">12dp</item>
    </style>

    <style name="CSTextInputFillWrap" parent="CSTextInputMatchWrap">
        <item name="android:layout_gravity">center|top</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>


    <style name="CSInputTextPickerMatchWrap" parent="CSInputTextMatchWrap">
        <item name="android:inputType">textFilter|textMultiLine|textNoSuggestions</item>
        <item name="android:cursorVisible">false</item>
        <item name="android:focusable">false</item>
        <item name="android:focusableInTouchMode">false</item>
        <item name="android:foreground">?android:attr/selectableItemBackground</item>
    </style>

    <style name="CSContainerEmptyText" parent="CSWrap">
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:textAppearance">@style/CSTextOverline</item>
    </style>

    <style name="CSTextInputMatchWrap" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CSTextInputWrap" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CSTextInputDropdownMatchWrap" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="CSInputTextMatchWrap" parent="Widget.MaterialComponents.TextInputEditText.OutlinedBox">
        <item name="android:inputType">textFilter|textMultiLine|textNoSuggestions</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="CSBorderLeft">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_gravity">start</item>
        <item name="android:background">?attr/colorPrimaryVariant</item>
    </style>

    <style name="CSBorderRight">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_gravity">end</item>
        <item name="android:background">?attr/colorPrimaryVariant</item>
    </style>

</resources>
